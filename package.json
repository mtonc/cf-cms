{
	"name": "cf-cms",
	"description": "Basic CMS for static sites backed by Cloudflare",
	"private": false,
	"cloudflare": {
		"label": "Worker + D1 Database",
		"products": [
			"Workers",
			"D1"
		],
		"categories": [
			"storage"
		],
		"docs_url": "https://developers.cloudflare.com/d1/",
		"preview_icon_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/532a5378-9717-4964-385b-5fafd2160200/public",
		"preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/ec7f4be5-d1d6-4e94-0851-126341745400/preview",
		"publish": true
	},
	"dependencies": {
		"@nanostores/react": "github:nanostores/react",
		"hono": "4.8.2",
		"nanostores": "^1.0.1",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"styled-components": "^6.1.19"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "1.7.5",
		"@eslint/js": "9.29.0",
		"@types/node": "^24.10.0",
		"@types/react": "19.0.10",
		"@types/react-dom": "19.0.4",
		"@vitejs/plugin-react": "5.0.0",
		"css-loader": "^7.1.2",
		"eslint": "9.27.0",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-react-refresh": "0.4.20",
		"globals": "16.2.0",
		"typescript": "5.8.3",
		"typescript-eslint": "8.35.0",
		"vite": "^6.0.0",
		"wrangler": "^4.43.0"
	},
	"scripts": {
		"build": "tsc -b && vite build",
		"cf-typegen": "wrangler types",
		"check": "tsc && vite build && wrangler deploy --dry-run",
		"deploy": "wrangler deploy",
		"lint": "eslint .",
		"preview": "npm run build && vite preview",
		"dev": "npm run seedLocalD1 && vite",
		"predeploy": "wrangler d1 migrations apply DB --remote",
		"seedLocalD1": "wrangler d1 migrations apply DB --local"
	},
	"type": "module"
}
